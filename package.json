    "name": "nr1-network-telemetry-nerdpack",
    "description": "Nerdpack nr1-network-telemetry-nerdpack",
    "version": "0.1.0",
    "scripts": {
        "checkversion": "check-node-version --node $(cat .nvmrc)",
        "format": "prettier \"**/*.js\" \"**/*.json\" \"**/*.jsx\" \"**/*.scss\" --ignore-path .eslintignore --list-different",
        "format:fix": "npm run format -- --write",
        "lint": "npm-run-all --parallel format lint:*",
        "lint:css": "stylelint 'nerdlets/**/*.scss' --syntax scss",
        "lint:css:fix": "npm run lint:css -- --fix",
        "lint:js": "eslint --ignore-path .eslintignore .",
        "lint:js:fix": "npm run lint:js -- --fix",
        "lint:ts": "tsc",
        "start": "nr1 nerdpack:serve",
        "test": "npm-run-all --sequential checkversion lint test:js",
        "test:js": "jest"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
            "pre-push": "npm run test"
        }
    },
    "bugs": {
